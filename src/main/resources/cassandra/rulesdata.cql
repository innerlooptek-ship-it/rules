INSERT  INTO iqe.questionnaire_rules (rule_id, flow, action, condition, lob, rule_name, salience)
VALUES ('14', 'VM', '"{\n  \"questions\": [\n    {\n      \"id\": \"TB_reason\",\n      \"text\": \"Why are you getting a tuberculosis (TB) test today?\",\n      \"errorMessage\": \"Select all that apply.\",\n      \"answerType\": \"checkbox\",\n      \"required\": true,\n      \"answerOptions\": [\n        {\n          \"text\": \"My employer requires it\",\n          \"value\": \"employer_requirement\"\n        },\n        {\n          \"text\": \"I’m a healthcare worker\",\n          \"value\": \"healthcare_worker\"\n        },\n        {\n          \"text\": \"My school requires it\",\n          \"value\": \"school_requirement\"\n        },\n        {\n          \"text\": \"I had contact with someone with TB\",\n          \"value\": \"TB_contact\"\n        },\n        {\n          \"text\": \"I visited an area with a high TB rate\",\n          \"value\": \"high_TB_rate\"\n        },\n        {\n          \"text\": \"I require it for immigration reasons\",\n          \"value\": \"immigration_reason\"\n        },\n        {\n          \"text\": \"I’m a resident or employee at a correctional facility, nursing home, homeless shelter, hospital or other health care facility\",\n          \"value\": \"health_care_facility\"\n        },\n        {\n          \"text\": \"I’m an intravenous (IV) drug user\",\n          \"value\": \"IV_drug_user\"\n        },\n        {\n          \"text\": \"Other\",\n          \"value\": \"other\"\n        }\n      ]\n    },\n    {\n      \"id\": \"TB_symptoms\",\n      \"text\": \"Do you have any of the following symptoms?\",\n      \"errorMessage\": \"Select all that apply.\",\n      \"answerType\": \"checkbox\",\n      \"required\": true,\n      \"answerOptions\": [\n        {\n          \"text\": \"Fever\",\n          \"value\": \"fever\"\n        },\n        {\n          \"text\": \"Coughing blood\",\n          \"value\": \"coughing_blood\"\n        },\n        {\n          \"text\": \"Coughing for longer than 3 weeks\",\n          \"value\": \"coughing_3_weeks\"\n        },\n        {\n          \"text\": \"Chest pain\",\n          \"value\": \"chest_pain\"\n        },\n        {\n          \"text\": \"Unintentional weight loss\",\n          \"value\": \"weight_loss\"\n        },\n        {\n          \"text\": \"Night sweats\",\n          \"value\": \"night_sweat\"\n        },\n        {\n          \"text\": \"No symptoms\",\n          \"value\": \"no_symptoms\"\n        }\n      ]\n    },\n    {\n      \"id\": \"TB_history\",\n      \"text\": \"Check if you''ve ever had the following:\",\n      \"errorMessage\": \"Select all that apply.\",\n      \"answerType\": \"checkbox\",\n      \"required\": true,\n      \"answerOptions\": [\n        {\n          \"text\": \"BCG vaccine\",\n          \"value\": \"bcg_vaccine\"\n        },\n        {\n          \"text\": \"Positive test for TB\",\n          \"value\": \"positive_test\"\n        },\n        {\n          \"text\": \"Serious reaction to TB skin test\",\n          \"value\": \"serious_reaction\"\n        },\n        {\n          \"text\": \"TB diagnosis or treatment for TB in the past\",\n          \"value\": \"TB_diagnosis_treatment\"\n        },\n        {\n          \"text\": \"Allergy to phenol\",\n          \"value\": \"phenol_allergy\"\n        },\n        {\n          \"text\": \"Flu, chickenpox, measles or mumps in the last 30 days\",\n          \"value\": \"flu_30days\"\n        },\n        {\n          \"text\": \"Another vaccine in the last 30 days\",\n          \"value\": \"other_vax_30days\"\n        },\n        {\n          \"text\": \"History of fainting after vaccine or injection\",\n          \"value\": \"fainting_history\"\n        },\n        {\n          \"text\": \"None of these apply\",\n          \"value\": \"none\"\n        }\n      ]\n    }\n  ]\n}"','requiredQuestionnaireContext == "TB"', 'CLINIC', 'VM_TB',100);